 
public class ContaNormal extends Conta {
  // Fields
  // Methods
  // Constructors
  // Accessor Methods
  // Operations
  /**
   * 
   * @param valor 
   */
      
  public void creditar (double valor) {
    saldo = saldo + valor;
  }
  /**
   * 
   * @param valor 
   */
  public void debitar (double valor) {  // Conta Normal não possui limite, logo em uma operação de débito apenas o saldo está disponível.
    if ((saldo-valor) >= 0) {
    	saldo = saldo - valor;
    }
	
  }
}

